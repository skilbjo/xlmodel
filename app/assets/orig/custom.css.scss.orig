/* miscellaneious */

  .debug_dump {
    clear: both;
    float: center;
    width: 50%;
    margin-top: 45px;
    @include box_sizing;
  }

/* forms */

  input, textarea, select, .uneditable-input {
    border: 1px solid #bbb;
    width: 50%;
    margin-bottom: 15px;
    @include box_sizing;
  }

  input {
    height: auto !important;
  }

#error_explanation {
  color: #f00;
  ul { list-style: none;
    margin: 0 0 18px 0;
  }
}

.field_with_errors {
  @extend .control-group;
  @extend .error;
}

